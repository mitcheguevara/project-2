<h2><%= @dog.name %></h2>

 <%= form_for [@owner, @dog] do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %>
 <br>
 <%= f.label :breed %>
 <%= f.text_field :breed %>
 <br>
 <%= f.label :likes_dogs %>
 <%= f.text_field :likes_dogs %>
 <br>
 <%= f.label :dog_img %>
 <%= f.text_field :dog_img %>
 <br>
   <%= f.submit "Update" %> <%= link_to "Cancel", owner_dog_path(@owner, @dog) %>
 <% end %>



<%= link_to 'You are going up for adoption!', owner_dog_path(@owner, @dog) %>
